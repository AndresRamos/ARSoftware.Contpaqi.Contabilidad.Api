          {
            "name": "Ocp-Apim-Subscription-Key",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-Empresa-Rfc",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "La estructura de una solicitud serializada en JSON.",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/BuscarCuentasRequest"
                    },
                    {
                      "$ref": "#/components/schemas/CrearCuentaRequest"
                    },
                    {
                      "$ref": "#/components/schemas/BuscarDiariosEspecialesRequest"
                    },
                    {
                      "$ref": "#/components/schemas/BuscarEmpresasRequest"
                    },
                    {
                      "$ref": "#/components/schemas/BuscarPolizasRequest"
                    },
                    {
                      "$ref": "#/components/schemas/CrearPolizaRequest"
                    },
                    {
                      "$ref": "#/components/schemas/EliminarPolizaRequest"
                    },
                    {
                      "$ref": "#/components/schemas/BuscarSegmentosNegocioRequest"
                    },
                    {
                      "$ref": "#/components/schemas/BuscarTiposPolizaRequest"
                    }
                  ]
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
