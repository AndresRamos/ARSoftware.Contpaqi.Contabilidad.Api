{
  "openapi": "3.0.1",
  "info": {
    "title": "AR Software - CONTPAQi Contabilidad API",
    "description": "API used to create request to be processed in CONTPAQi Contabilidad",
    "contact": {
      "name": "AR Software",
      "url": "https://www.arsoft.net",
      "email": "andres@arsoft.net"
    },
    "version": "v1"
  },
  "paths": {
    "/api/Requests/{id}": {
      "get": {
        "tags": [
          "Requests"
        ],
        "summary": "Busca una solicitud por id.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Id de la solicitud a buscar.",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "Ocp-Apim-Subscription-Key",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-Empresa-Rfc",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Retorna la solicitud.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiRequest"
                }
              }
            }
          },
          "404": {
            "description": "No se encontro la solicitud.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud invalida.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/api/Requests": {
      "get": {
        "tags": [
          "Requests"
        ],
        "summary": "Busca solicitudes por rango de fecha.",
        "parameters": [
          {
            "name": "startDate",
            "in": "query",
            "description": "Fecha inicio.",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "Fecha fin.",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "Ocp-Apim-Subscription-Key",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-Empresa-Rfc",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Coleccion de solicitudes dentro rango de fechas.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiRequest"
                }
              }
            }
          },
          "204": {
            "description": "No hay solicitudes dentro del rango de fecha."
          },
          "400": {
            "description": "Solicitud invalida.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Requests"
        ],
        "summary": "Crear una solicitud en la base de datos para ser procesada en CONTPAQi Comercial.",
        "parameters": [
          {
            "name": "Ocp-Apim-Subscription-Key",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "x-Empresa-Rfc",
            "in": "header",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Solicitud a procesar.",
          "content": {
            "application/json": {
              "schema": {
                "oneOf": [
                  {
                    "$ref": "#/components/schemas/BuscarCuentasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearCuentaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarDiariosEspecialesRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarEmpresasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarPolizasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/EliminarPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarSegmentosNegocioRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarTiposPolizaRequest"
                  }
                ]
              }
            },
            "text/json": {
              "schema": {
                "oneOf": [
                  {
                    "$ref": "#/components/schemas/BuscarCuentasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearCuentaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarDiariosEspecialesRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarEmpresasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarPolizasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/EliminarPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarSegmentosNegocioRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarTiposPolizaRequest"
                  }
                ]
              }
            },
            "application/*+json": {
              "schema": {
                "oneOf": [
                  {
                    "$ref": "#/components/schemas/BuscarCuentasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearCuentaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarDiariosEspecialesRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarEmpresasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarPolizasRequest"
                  },
                  {
                    "$ref": "#/components/schemas/CrearPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/EliminarPolizaRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarSegmentosNegocioRequest"
                  },
                  {
                    "$ref": "#/components/schemas/BuscarTiposPolizaRequest"
                  }
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Retorna la solicitud creada.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "uuid"
                }
              }
            }
          },
          "400": {
            "description": "Solicitud invalida.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/api/Requests/Pending": {
      "get": {
        "tags": [
          "Requests"
        ],
        "summary": "Busca las solicitudes pendientes por procesar.",
