          }
        ],
        "responses": {
          "200": {
            "description": "Retorna la respuesta.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiResponse"
                }
              }
            }
          },
          "404": {
            "description": "No se encontro la respuesta.",
