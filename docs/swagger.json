          {
            "name": "id",
            "in": "path",
            "description": "Id de la respuesta a buscar.",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "Ocp-Apim-Subscription-Key",
            "in": "header",
            "schema": {
              "type": "string"
